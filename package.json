{
  "name": "capacitor-ibeacon",
  "version": "0.1.0",
  "description": "IBeacon Transmit and Scan",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "author": "Benjamin Reed <github@raccoonfink.com>",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
