    "@typescript-eslint/parser": "^2.18.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.8.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorIbeacon.podspec"
